#include <stdio.h>
#include "memIO.h"
#define MEM_BASE 0x43c00000

int main()
{
    printf("Hello World\n");
	writeMem(MEM_BASE, 'w', 0x01020304);
	writeMem(MEM_BASE+4, 'w', 0x04030201);


	readMem(MEM_BASE+8, 'w');
	readMem(MEM_BASE+12, 'w');

    return 0;
}
